<!--miniprogram/pages/zhuce/zhuce.wxml-->
<van-nav-bar title="注册" left-text="返回" right-text="登录" left-arrow bind:click-left="onClickLeft" bind:click-right="onClickRight" custom-class="title" title-class="ziti"  />
<van-cell-group>
  <van-field value="{{ username }}" required clearable label="用户名" icon="question-o" placeholder="请输入用户名" bind:click-icon="onClickIcon" bind:blur="onUsername"/>

  <van-field value="{{ password }}" type="password" label="密码" placeholder="请输入密码" required border="{{ false }}"  bind:blur="onBlur"/>
  <van-field value="{{ password }}" type="password" label="确认密码" placeholder="请确认密码" required border="{{ false }}" bind:blur="onChange" bind:blur="onqueren" />
</van-cell-group>
<button class="zhuce" bindtap="zhuce">注册</button>
<button class="denglu" bindtap="denglu">前往登录</button>
