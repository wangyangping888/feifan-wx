<!--miniprogram/pages/denglu/denglu.wxml-->
<van-nav-bar title="登录" left-text="返回" right-text="注册" left-arrow bind:click-left="onClickLeft" bind:click-right="onClickRight" custom-class="title" title-class="ziti"  />
<van-cell-group>
  <van-field value="{{ username }}" required clearable label="用户名" icon="question-o" placeholder="请输入用户名" bind:click-icon="onClickIcon" bind:blur="onUsername"/>

  <van-field value="{{ password }}" type="password" label="密码" placeholder="请输入密码" required border="{{ false }}" bind:blur="onChange" />
</van-cell-group>
<button class="denglu" bindtap="daka">登录</button>
<button class="zhuce" bindtap="zhuce">前往注册</button>